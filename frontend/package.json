{
  "name": "react-ts-auth-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm --no-warnings=ExperimentalWarning server.ts && tsr generate && tsr watch",
    "build": "npm run build:client && npm run build:server.ts",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server.ts": "vite build --ssr src/server.tsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node --loader ts-node/esm --no-warnings=ExperimentalWarning server.ts",
    "server-json": "json-server --watch src/db/db.json --port 3000"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.14",
    "@tanstack/react-query-devtools": "^5.59.15",
    "@tanstack/react-router": "^1.69.1",
    "@tanstack/router": "^0.0.1-beta.53",
    "@tanstack/router-devtools": "^1.67.0",
    "@tanstack/router-vite-plugin": "^1.64.0",
    "@types/compression": "^1.7.5",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "boxicons": "^2.1.4",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "js-cookie": "^3.0.5",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.4.47",
    "postcss-load-config": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.0",
    "react-ts-auth-ssr": "file:",
    "sirv": "^2.0.4",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.64.0",
    "@types/express": "^4.17.21",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-checker": "^0.8.0"
  }
}
